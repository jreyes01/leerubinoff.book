

<div class="row">
  <div class="span4">

<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html:{class: "well"}) do |f| %>

<fieldset>
  <legend>Sign Up</legend>

  <%= devise_error_messages! %>
  <div><%= f.label :first_name %><br />
  	<%= f.text_field :first_name %></div>

  	 <div><%= f.label :last_name %><br />
  	<%= f.text_field :last_name %></div>

  	 <div><%= f.label :profile_name %><br />
  	<%= f.text_field :profile_name %></div>

  <div><%= f.label :email %><br />
  <%= f.email_field :email %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Sign up" %></div>

</fieldset>
<% end %>

<%= render "devise/shared/links" %>

</div>

<div class="span8">
  <h2>Registration is only restricted to <br> Lee Rubinoff & Justin Reyes ! <br> Hence, Secret Registration...</h2>
  <img src="http://i1286.photobucket.com/albums/a603/zucchiniman3/the-social-network-eduardo-mark_zps396d46f5.jpg">
</div>

</div>


